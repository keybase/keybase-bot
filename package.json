{
  "name": "keybase-bot",
  "version": "2.0.5",
  "description": "Script Keybase in Node.js!",
  "keywords": [
    "keybase",
    "kbfs",
    "bot",
    "chatbot",
    "encryption",
    "crypto",
    "pgp",
    "gpg"
  ],
  "main": "lib/index.js",
  "homepage": "https://github.com/keybase/keybase-bot",
  "repository": "git@github.com:keybase/keybase-bot.git",
  "bugs": {
    "url": "https://github.com/keybase/keybase-bot/issues"
  },
  "author": "Chris Coyne <ccoyne77@gmail.com>",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "lib/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "docs": "documentation readme lib/** --config documentation.yml --section=API",
    "tsc": "tsc",
    "test": "jest --runInBand",
    "release": "standard-version"
  },
  "dependencies": {
    "@typescript-eslint/parser": "1.10.2",
    "lodash.camelcase": "4.3.0",
    "lodash.kebabcase": "4.1.1",
    "lodash.snakecase": "4.1.1",
    "mkdirp": "0.5.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.14",
    "@types/lodash.camelcase": "4.3.6",
    "@types/lodash.kebabcase": "4.1.6",
    "@types/lodash.snakecase": "4.1.6",
    "@types/mkdirp": "0.5.2",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "documentation": "9.1.1",
    "eslint": "5.9.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsdoc": "3.14.1",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "1.2.0",
    "jest": "24.8.0",
    "lint-staged": "8.1.0",
    "mathjs": "5.2.3",
    "prettier": "1.18.2",
    "regenerator-runtime": "0.13.1",
    "standard-version": "4.4.0",
    "ts-jest": "24.0.2",
    "typescript": "3.5.1"
  }
}
